{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": { "no-console": false },
  "rules": {
    "no-unused-variable": true,
    "no-var-requires": false,
    "semicolon": [true, "never"],
    "member-access": false,
    "no-console": [false]
  },
  "rulesDirectory": []
}
